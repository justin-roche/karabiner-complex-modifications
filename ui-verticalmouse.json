{
    "title": "ui > scroll",
    "rules": [
        {
            "description": "ui > scroll",
            "manipulators": [
                {
                    "type": "basic",
                    "from": {
                        "key_code": "u",
                        "modifiers": {
                            "optional": [
                                "any"
                            ]
                        }
                    },
                    "to": [
                        {
                            "mouse_key": {
                                "vertical_wheel": 64
                            }
                        }
                    ]
                },
                {
                    "type": "basic",
                    "from": {
                        "key_code": "i",
                        "modifiers": {
                            "optional": [
                                "any"
                            ]
                        }
                    },
                    "to": [
                        {
                            "mouse_key": {
                                "vertical_wheel": -64
                            }
                        }
                    ]
                }
            ]
        }
    ]
}
